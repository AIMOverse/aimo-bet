# ==============================================================================
# Environment Configuration
# ==============================================================================
# Copy this file to .env.local and fill in your actual values
# Never commit .env.local to version control
# ==============================================================================

# ------------------------------------------------------------------------------
# Supabase Configuration
# ------------------------------------------------------------------------------
# Database and authentication backend
# Get these values from your Supabase project settings

NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=
SUPABASE_SERVICE_ROLE_KEY=

# ------------------------------------------------------------------------------
# AI Provider Configuration
# ------------------------------------------------------------------------------
# Set which AI provider to use: "aimo" or "openrouter"
# If not set, defaults to "aimo"

AI_PROVIDER=

# OpenRouter - Multi-model API gateway (required if AI_PROVIDER=openrouter)
OPENROUTER_API_KEY=

# DFlow - Prediction markets data
DFLOW_API_KEY=

# Exa - Web search API
EXA_API_KEY=

# ------------------------------------------------------------------------------
# PartyKit Configuration
# ------------------------------------------------------------------------------
# Real-time WebSocket server for live price updates
# Example: your-project.partykit.dev

NEXT_PUBLIC_PARTYKIT_HOST=

# ------------------------------------------------------------------------------
# AI Agent Wallets (Required for trading)
# ------------------------------------------------------------------------------
# Each model series needs SVM (Solana) wallet keys for aimo-network payments
# Generate with: npx tsx scripts/generateKeys.ts && npx tsx scripts/listKeys.ts --env
#
# Naming convention:
#   WALLET_<SERIES>_SVM_PUBLIC  - Solana public key (for display/filtering)
#   WALLET_<SERIES>_SVM_PRIVATE - Solana private key (for aimo-network signing)
#   WALLET_<SERIES>_EVM_PRIVATE - Polygon private key (optional, for Polymarket)

WALLET_GPT_SVM_PUBLIC=
WALLET_GPT_SVM_PRIVATE=
WALLET_CLAUDE_SVM_PUBLIC=
WALLET_CLAUDE_SVM_PRIVATE=
WALLET_GEMINI_SVM_PUBLIC=
WALLET_GEMINI_SVM_PRIVATE=
WALLET_GROK_SVM_PUBLIC=
WALLET_GROK_SVM_PRIVATE=
WALLET_DEEPSEEK_SVM_PUBLIC=
WALLET_DEEPSEEK_SVM_PRIVATE=
WALLET_GLM_SVM_PUBLIC=
WALLET_GLM_SVM_PRIVATE=
WALLET_QWEN_SVM_PUBLIC=
WALLET_QWEN_SVM_PRIVATE=
WALLET_KIMI_SVM_PUBLIC=
WALLET_KIMI_SVM_PRIVATE=

# ------------------------------------------------------------------------------
# Development & Testing (Optional)
# ------------------------------------------------------------------------------
# Solana wallet credentials for testing (DO NOT use production keys)

TEST_WALLET_PUBLIC_KEY=
TEST_WALLET_PRIVATE_KEY=
