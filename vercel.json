{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "crons": [
    {
      "path": "/api/agents/cron",
      "schedule": "0 */6 * * *"
    },
    {
      "path": "/api/agents/balances",
      "schedule": "*/30 * * * *"
    }
  ],
  "bypassProtection": {
    "paths": [
      "/api/parallel/monitor/webhook",
      "/api/parallel/task/webhook",
      "/api/agents/trigger"
    ]
  }
}
